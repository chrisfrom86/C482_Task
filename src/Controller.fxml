        <rowConstraints>
          <RowConstraints maxHeight="173.0" minHeight="0.0" prefHeight="21.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="466.0" minHeight="10.0" prefHeight="445.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="1">
               <children>
                  <TableView fx:id="partsTable" prefHeight="466.0" prefWidth="375.0">
                    <columns>
                      <TableColumn fx:id="partIDColumn" prefWidth="50.0" text="Part ID" />
                      <TableColumn fx:id="partNameColumn" prefWidth="120.0" text="Part Name" />
                      <TableColumn fx:id="partInventoryColumn" prefWidth="68.0" text="Inventory" />
                      <TableColumn fx:id="partPriceColumn" prefWidth="130.0" text="Price/Cost per Unit" />
                    </columns>
                     <padding>
                        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                     </padding>
                  </TableView>
               </children>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <TableView fx:id="productTable" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn fx:id="productIDColumn" prefWidth="68.0" text="Product ID" />
                        <TableColumn fx:id="productNameColumn" prefWidth="120.0" text="Product Name" />
                        <TableColumn fx:id="productInventoryColumn" prefWidth="68.0" text="Inventory" />
                        <TableColumn fx:id="productPriceColumn" prefWidth="130.0" text="Price/Cost per Unit" />
                    </columns>
                     <padding>
                        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                     </padding>
                  </TableView>
               </children>
            </StackPane>
            <HBox alignment="CENTER_LEFT" nodeOrientation="RIGHT_TO_LEFT" spacing="10.0" GridPane.rowIndex="2">
               <children>
                  <Button fx:id="deletePart" mnemonicParsing="false" onAction="#deletePart" text="Delete" />
                  <Button fx:id="updatePart" mnemonicParsing="false" onAction="#updatePart" text="Modify" />
                  <Button fx:id="addPart" mnemonicParsing="false" onAction="#addPart" text="Add" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <Button fx:id="deleteProduct" mnemonicParsing="false" onAction="#onDeleteProduct" text="Delete" />
                  <Button fx:id="updateProduct" mnemonicParsing="false" onAction="#updateProduct" text="Modify" />
                  <Button fx:id="addProduct" mnemonicParsing="false" onAction="#addProduct" text="Add" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="75.0">
               <children>
                  <TextField fx:id="partSearch" onAction="#lookupPart" promptText="Search by ID or Name" />
                  <Label text="Parts">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="75.0" GridPane.columnIndex="1">
               <children>
                  <TextField fx:id="productSearch" onAction="#lookupProduct" promptText="Search by ID or Name" />
                  <Label text="Products">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
